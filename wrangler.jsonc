/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "not-sure",
	"main": "src/index.ts",
	"compatibility_date": "2025-11-21",
	"compatibility_flags": [
		"global_fetch_strictly_public"
	],
	"assets": {
		// The path to the directory containing the `index.html` file to be served at `/`
		"directory": "./public"
	},
	"observability": {
		"enabled": true,
	},
	"kv_namespaces": [
		{
			"binding": "RAG_KV",
			"id": "f6f89996bdfc40048e31ed392167db32"
		}
	],
	"r2_buckets": [
		{
			"binding": "not_sure_assets",
			"bucket_name": "not-sure-assets"
		}
	],
	"ai": {
		"binding": "AI"
	},
	/**
	 * Smart Placement
	 * Docs: https://developers.cloudflare.com/workers/configuration/smart-placement/#smart-placement
	 */
	// "placement": { "mode": "smart" }
	/**
	 * Bindings
	 * Bindings allow your Worker to interact with resources on the Cloudflare Developer Platform, including
	 * databases, object storage, AI inference, real-time communication and more.
	 * https://developers.cloudflare.com/workers/runtime-apis/bindings/
	 */
	/**
	 * Environment Variables
	 * https://developers.cloudflare.com/workers/wrangler/configuration/#environment-variables
	 */
	"vars": {
		"ACCOUNT_ID": "1d361f061ebf3d1a293900bdb815db26",
		"AI_GATEWAY_ID": "AQ.Ab8RN6JKBAPQhVWbuiBKBT1j9r3lE0Gqj4rfeat0SIo9aQzFFw",
		"AI_GATEWAY_URL": "https://gateway.ai.cloudflare.com/v1",
		// Set to "true" if using a custom domain that maps directly to the gateway provider path
		"AI_GATEWAY_SKIP_PATH_CONSTRUCTION": "false",
		// Vertex AI Configuration (via Cloudflare AI Gateway)
		// "GCP_PROJECT_ID": "your-gcp-project-id" (same as GitHub secret GCP_PROJECT_ID)
		// "VERTEX_AI_LOCATION": "us-central1"
		// "VERTEX_AI_MODEL": "gemini-1.5-flash"
		// Note: VERTEX_AI_SERVICE_ACCOUNT_JSON should be set as a secret via:
		// wrangler secret put VERTEX_AI_SERVICE_ACCOUNT_JSON
		// See VERTEX_AI_SETUP.md for complete setup instructions
	},
	/**
	 * Routes
	 * https://developers.cloudflare.com/workers/wrangler/configuration/#routes
	 */
	"routes": [
		{
			"pattern": "lornu.ai",
			"custom_domain": true
		},
		{
			"pattern": "www.lornu.ai",
			"custom_domain": true
		}
	]
	/**
	 * Note: Use secrets to store sensitive data.
	 * https://developers.cloudflare.com/workers/configuration/secrets/
	 */
	/**
	 * Static Assets
	 * https://developers.cloudflare.com/workers/static-assets/binding/
	 */
	// "assets": { "directory": "./public/", "binding": "ASSETS" }
	/**
	 * Service Bindings (communicate between multiple Workers)
	 * https://developers.cloudflare.com/workers/wrangler/configuration/#service-bindings
	 */
	// "services": [{ "binding": "MY_SERVICE", "service": "my-service" }]
}